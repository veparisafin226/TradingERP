@model IEnumerable<TradingERP.Models.DieselMaster>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid">

<h1>Diesel Entry</h1>

    <p class="float-right btn btn-primary">
